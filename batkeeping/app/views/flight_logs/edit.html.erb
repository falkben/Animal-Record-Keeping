<h1>Editing flight_logs</h1>

<% form_for(@flight_logs) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :bat_id %><br />
    <%= f.text_field :bat_id %>
  </p>
  <p>
    <%= f.label :date %><br />
    <%= f.datetime_select :date %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </p>
  <p>
    <%= f.label :temperature %><br />
    <%= f.text_field :temperature %>
  </p>
  <p>
    <%= f.label :humidity %><br />
    <%= f.text_field :humidity %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @flight_logs %> |
<%= link_to 'Back', flight_logs_path %>
