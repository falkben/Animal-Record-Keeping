<h1>Listing flight_logs</h1>

<table>
  <tr>
    <th>Bat</th>
    <th>Date</th>
    <th>Note</th>
    <th>Temperature</th>
    <th>Humidity</th>
  </tr>

<% for flight_logs in @flight_logs %>
  <tr>
    <td><%=h flight_logs.bat_id %></td>
    <td><%=h flight_logs.date %></td>
    <td><%=h flight_logs.note %></td>
    <td><%=h flight_logs.temperature %></td>
    <td><%=h flight_logs.humidity %></td>
    <td><%= link_to 'Show', flight_logs %></td>
    <td><%= link_to 'Edit', edit_flight_logs_path(flight_logs) %></td>
    <td><%= link_to 'Destroy', flight_logs, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New flight_logs', new_flight_log_path %>
