<h1>Editing cage <%= @cage.name %></h1>

<% @disable_cage_name_change = true %>

<%= start_form_tag :action => 'update', :id => @cage %>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Edit' %>
<%= end_form_tag %>

<%= link_to 'Show', :action => 'show', :id => @cage %> |
<%= link_to 'Back', :action => 'list' %>
