<h1>Listing flight_objects</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for flight_objects in @flight_objects %>
  <tr>
    <td><%=h flight_objects.name %></td>
    <td><%= link_to 'Show', flight_objects %></td>
    <td><%= link_to 'Edit', edit_flight_objects_path(flight_objects) %></td>
    <td><%= link_to 'Destroy', flight_objects, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New flight_objects', new_flight_objects_path %>
