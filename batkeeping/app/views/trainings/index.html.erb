<h1>Listing trainings</h1>

<table>
  <tr>
    <th>Training type</th>
    <th>User</th>
    <th>Date</th>
    <th>User trained by</th>
    <th>Note</th>
  </tr>

<% for training in @trainings %>
  <tr>
    <td><%=h training.training_type_id %></td>
    <td><%=h training.user_id %></td>
    <td><%=h training.date %></td>
    <td><%=h training.user_trained_by_id %></td>
    <td><%=h training.note %></td>
    <td><%= link_to 'Show', training %></td>
    <td><%= link_to 'Edit', edit_training_path(training) %></td>
    <td><%= link_to 'Destroy', training, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New training', new_training_path %>
