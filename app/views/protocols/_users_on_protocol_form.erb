<%#
# checkboxes for users on protocol
%>

<table class="centered">
  <th></th>
  <th style="font-size:10pt">Name</th>
<% for u in users %>
  <tr>
    <td><%= check_box :user_id, u.id, :checked => protocol.users.include?(u)  %></td>
    <td><label for='user_id_<%=u.id %>' ><%= u.name %></label></td>
  </tr>
<% end %>
</table>
